let result = rankedBalance(10, 4);

function rankedBalance(victories, defeats){
    let balance = victories - defeats;
    return balance;
}

if (result <= 10) {
    console.log("O Herói tem de saldo de vitórias " + result + " e está no nível de Iron");
}
else if (result >= 11 && result <= 20){
    console.log("O Herói tem de saldo de vitórias " + result + " e está no nível de Bronze");
}else if (result >= 21 && result <= 50){
    console.log("O Herói tem de saldo de vitórias " + result + " e está no nível de Silver");
}else if (result >= 51 && result <= 80){
    console.log("O Herói tem de saldo de vitórias " + result + " e está no nível de Gold");
}else if (result >= 81 && result <= 90){
    console.log("O Herói tem de saldo de vitórias " + result + " e está no nível de Diamond");
}else if (result >= 91 && result <= 100){
    console.log("O Herói tem de saldo de vitórias " + result + " e está no nível de Legendary");
}else{
    console.log("O Herói tem de saldo de vitórias " + result + " e está no nível de Immortal");
}